# Frontend environment example for Ocean Notes (used by hosting environment)
# The app falls back to localStorage if API URLs are not set.

NG_APP_API_BASE=
NG_APP_BACKEND_URL=
NG_APP_FRONTEND_URL=
NG_APP_WS_URL=
NG_APP_NODE_ENV=development
NG_APP_ENABLE_SOURCE_MAPS=true
NG_APP_PORT=3000
NG_APP_TRUST_PROXY=true
NG_APP_LOG_LEVEL=info
NG_APP_HEALTHCHECK_PATH=/healthz
NG_APP_FEATURE_FLAGS=
NG_APP_EXPERIMENTS_ENABLED=false
